<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".Journal_entry">


    <TextView
        android:id="@+id/Calories_Remaining"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="calories remaining"
        android:textSize="60sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/Breakfast"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="breakfast"
        android:textSize="45sp"
        tools:layout_editor_absoluteX="-6dp"
        tools:layout_editor_absoluteY="161dp"
        tools:ignore="MissingConstraints" />


    <ListView
        android:id="@+id/Output_Breakfast"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:padding="10dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="331dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/Input_Food"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="add_food"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="-5dp"
        tools:layout_editor_absoluteY="281dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:hint="type_food_here"
        android:id="@+id/Input_Breakfast"
        tools:layout_editor_absoluteX="-5dp"
        tools:layout_editor_absoluteY="236dp"
        tools:ignore="MissingConstraints" />




</androidx.constraintlayout.widget.ConstraintLayout>